{
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "core-js": "^3.11.1",
    "es6-promise": "^4.2.8",
    "node-fetch": "^2.6.1",
    "vue": "^2.6.12",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-plugin-vuex": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/test-utils": "^1.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chance": "^1.1.7",
    "cwd": "^0.10.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.9.0",
    "fetch-mock-jest": "^1.5.1",
    "flush-promises": "^1.0.2",
    "jest": "^26.6.3",
    "mustache": "^4.2.0",
    "strip-ansi": "^6.0.0",
    "striptags": "^3.1.1",
    "vue-jest": "^3.0.7"
  },
  "jest": {
    "testFailureExitCode": 0,
    "testMatch": [
      "**/test.*.js"
    ],
    "testSequencer": "./test/jest-tuwien/sequencer.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "reporters": [
      [
        "./test/jest-tuwien/reporter.js",
        {
          "title": "A4",
          "maxPoints": 15,
          "defaultMinusPoints": 0.5
        }
      ]
    ],
    "globals": {
      "__SEED__": 1
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": 1
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
